{
  "name": "sushi-web3-react",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.2",
  "description": "A simple, maximally extensible, dependency minimized framework for building modern Ethereum dApps",
  "keywords": [
    "react",
    "react-hooks",
    "hooks",
    "ethereum",
    "javascript",
    "typescript",
    "web3",
    "context",
    "frontend",
    "dapp",
    "metamask",
    "walletconnect",
    "walletlink",
    "frame",
    "fortmatic",
    "portis",
    "squarelink",
    "torus",
    "authereum",
    "magic"
  ],
  "author": "Jiro Ono",
  "repository": {
    "type": "git",
    "url": "git://github.com/sushiswap/sushi-web3-react"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "publish:lerna": "lerna publish --no-git-tag-version --no-push",
    "clean": "lerna clean --yes && lerna exec -- rimraf yarn.lock dist/ .rts2_cache_{cjs,esm}/",
    "build": "lerna run build",
    "start": "lerna run --parallel --no-bail start -- --noClean",
    "lint": "lerna run lint --parallel",
    "test": "lerna run test --parallel"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "lerna": "^3.22.1",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "rimraf": "^3.0.1",
    "tsdx": "^0.11.0"
  },
  "license": "GPL-3.0-or-later",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
